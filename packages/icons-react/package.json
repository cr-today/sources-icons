{
  "name": "@fex.to/provider-icons-react",
  "version": "3.0.3",
  "description": "A set of sources icons-react.",
  "author": "dejurin",
  "license": "MIT",
  "homepage": "https://fex.to",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fex-to/provider-icons.git",
    "directory": "packages/icons-react"
  },
  "scripts": {
    "build": "npm run clean && npm run copy:license && npm run build:icons && npm run build:bundles",
    "build:icons": "node build.mjs",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf ./src/icons/*.js && rm -rf stats",
    "test": "cd ../.. && npm run build:copy && cd - && npm run clean && npm run build:icons && jest --env=jsdom"
  },
  "main": "dist/cjs/provider-icons-react.js",
  "main:umd": "dist/umd/provider-icons-react.js",
  "module": "dist/esm/provider-icons-react.js",
  "unpkg": "dist/umd/provider-icons-react.min.js",
  "typings": "dist/provider-icons-react.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fex.to/provider-icons": "*",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@testing-library/react": "^11.2.6",
    "babel-jest": "^29.4.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^16.5.1 || ^17.0.0 || ^18.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
