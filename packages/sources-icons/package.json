{
  "name": "@cr.today/sources-icons",
  "version": "0.5.0",
  "description": "A set of sources icons.",
  "author": "dejurin",
  "license": "MIT",
  "homepage": "https://cr.today",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cr-today/sources-icons.git",
    "directory": "packages/sources-icons"
  },
  "main": "dist/cjs/sources-icons.js",
  "main:umd": "dist/umd/sources-icons.js",
  "exports": {
    "./*": [
      "./icons/*"
    ]
  },
  "module": "dist/esm/sources-icons.js",
  "unpkg": "dist/umd/sources-icons.min.js",
  "typings": "dist/sources-icons.d.ts",
  "sideEffects": false,
  "files": [
    "dist/*",
    "sources-nodes.json",
    "icons/*",
    "sources-sprite.svg"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run copy && pnpm run build:icons && pnpm run build:es && pnpm run build:bundles",
    "build:icons": "node build.mjs",
    "build:es": "babel src -d dist/esm",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "copy": "pnpm run copy:license && pnpm run copy:icons",
    "copy:icons": "mkdir -p ./icons && cp ../../icons/*.svg ./icons/",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf src/icons/*.js && rm -rf icons",
    "test": "jest --env=jsdom"
  },
  "devDependencies": {
    "babel-jest": "^29.4.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1"
  },
  "jest": {
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "packageManager": "pnpm@7.17.0"
}