{
  "name": "@cr.today/sources-icons",
  "version": "0.4.0",
  "author": "dejurin",
  "license": "MIT",
  "description": "A set of sources icons.",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "test": "turbo run test",
    "clean": "turbo run clean",
    "build": "pnpm run optimize && pnpm run update && pnpm run build:svgs && pnpm run build:icons && pnpm run build:packages && pnpm run preview && pnpm run changelog-image && pnpm run zip && pnpm run update:readme",
    "optimize": "node ./.build/optimize.mjs",
    "update": "pnpm run update:icons-version",
    "update:icons-version": "node ./.build/update-icons-version.mjs",
    "update:readme": "node ./.build/update-readme.mjs",
    "import": "pnpm run build:clean && node ./.build/import.mjs && pnpm run optimize",
    "changelog": "node ./.build/changelog.mjs",
    "changelog-commit": "git add . && node ./.build/changelog-commit.mjs",
    "changelog-image": "node ./.build/changelog-image.mjs",
    "build:packages": "pnpm run build:react && pnpm run build:png",
    "build:clean": "rm -rf ./icons/*.svg",
    "build:icons": "pnpm --filter @cr.today/sources-icons build",
    "build:react": "pnpm --filter @cr.today/sources-icons-react build",
    "build:png": "pnpm --filter @cr.today/sources-icons-png build",
    "build:svgs": "pnpm run build:copy && node ./.build/build-svgs.mjs",
    "build:copy": "rm -rf ./icons && mkdir ./icons && cp ./src/_icons/* ./icons/",
    "preview": "pnpm run preview-icons",
    "preview-icons": "node ./.build/preview-icons.mjs",
    "zip": "node ./.build/zip-files.mjs",
    "release": "git pull && release-it --verbose"
  },
  "dependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@babel/cli": "^7.20.7",
    "@babel/core": "7.20.12",
    "@babel/parser": "7.20.15",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@release-it-plugins/workspaces": "^3.2.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "6.5.1",
    "@svgr/core": "6.5.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "adm-zip": "^0.5.10",
    "cheerio": "^1.0.0-rc.12",
    "csv-parser": "^3.0.0",
    "esbuild": "^0.17.10",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "html-minifier": "^4.0.0",
    "lodash.template": "4.5.0",
    "minimist": "^1.2.8",
    "node-sass": "8.0.0",
    "parse-svg-path": "^0.1.2",
    "prettier": "^2.8.4",
    "release-it": "15.6.0",
    "rollup": "3.17.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "svg-outline-stroke": "1.3.1",
    "svgo": "^3.0.2",
    "svgpath": "^2.6.0",
    "svgson": "^5.2.1"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": true
    },
    "npm": false,
    "hooks": {
      "after:bump": "pnpm run build --latest-version ${latestVersion} --new-version ${version} --verbose",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}.",
      "before:git:init": "pnpm install"
    },
    "git": {
      "addUntrackedFiles": true,
      "changelog": "pnpm run changelog --silent --latest-version ${latestTag}",
      "tagName": "v${version}",
      "_requireBranch": "master",
      "requireCommits": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  },
  "packageManager": "pnpm@7.17.0"
}